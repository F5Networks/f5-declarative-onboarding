{
    "schemaVersion": "1.0.0",
    "async": true,
    "class": "Device",
    "Common": {
        "class": "Tenant",
        "myVlan": {
            "class": "VLAN",
            "tag": 1234,
            "mtu": 1500,
            "interfaces": [
                {
                    "name": "1.1",
                    "tagged": true
                }
            ],
            "autoLastHop": "enabled"
        },
        "internal": {
            "class": "VLAN",
            "tag": 4094,
            "interfaces": [
                {
                    "name": "1.1",
                    "tagged": false
                }
            ],
            "autoLastHop": "default"
        },
        "mySelfIp": {
            "class": "SelfIp",
            "address": "10.148.75.46/24",
            "vlan": "myVlan",
            "allowService": [
                "tcp:80"
            ]
        },
        "myIpv6SelfIp": {
            "class": "SelfIp",
            "address": "::ffff:0a94:552e/ffff:ffff:0:0:0:0:0:0",
            "vlan": "myVlan",
            "allowService": [
                "tcp:80"
            ],
            "trafficGroup": "traffic-group-local-only"
        },
        "myRoute": {
            "class": "Route",
            "gw": "10.148.75.1",
            "network": "10.20.30.0/24",
            "mtu": 1500
        },
        "myLocalOnlyRoute": {
            "class": "Route",
            "target": "myVlan",
            "network": "default",
            "localOnly": true
        },
        "internal-self": {
            "class": "SelfIp",
            "address": "10.1.20.31/32",
            "vlan": "internal",
            "allowService": "default",
            "trafficGroup": "traffic-group-local-only"
        },
        "int_rt": {
            "class": "Route",
            "gw": "10.1.20.1",
            "network": "10.1.20.0/24",
            "mtu": 1460
        },
        "int_gw_interface": {
            "class": "Route",
            "target": "internal",
            "network": "10.1.20.1/32",
            "mtu": 1460
        },
        "mySystem": {
            "class": "System",
            "autoPhonehome": false
        },
        "myResolver": {
            "class": "DNS_Resolver",
            "routeDomain": "0",
            "forwardZones": [
                {
                    "name": "forward.net",
                    "nameservers": [
                        "10.10.10.10:53",
                        "::ffff:1414:1414.53"
                    ]
                }
            ]
        },
        "myIpv6Mirror": {
            "class": "MirrorIp",
            "primaryIp": "::ffff:0a94:552e",
            "secondaryIp": "any6"
        },
        "testRoutingAsPath1": {
            "class": "RoutingAsPath",
            "entries": [
                {
                    "name": 10,
                    "regex": "^65001 *"
                }
            ]
        },
        "testRoutingAsPath2": {
            "class": "RoutingAsPath",
            "entries": [
                {
                    "name": 10,
                    "regex": "^$"
                },
                {
                    "name": 20,
                    "regex": "^65005$"
                }
            ]
        },
        "testRoutingPrefixList1": {
            "class": "RoutingPrefixList",
            "entries": [
                {
                    "name": 10,
                    "action": "permit",
                    "prefix": "1111:2222::/127",
                    "prefixLengthRange": 128
                }
            ]
        },
        "testRoutingPrefixList2": {
            "class": "RoutingPrefixList",
            "entries": [
                {
                    "name": 20,
                    "action": "permit",
                    "prefix": "10.3.3.0/24",
                    "prefixLengthRange": 32
                }
            ]
        },
        "testRouteMap": {
            "class": "RouteMap",
            "entries": [
                {
                    "name": "33",
                    "action": "permit",
                    "match": {
                        "asPath": "testRoutingAsPath1",
                        "ipv4": {
                            "address": {
                                "prefixList": "testRoutingPrefixList2"
                            }
                        },
                        "ipv6": {
                            "address": {
                                "prefixList": "testRoutingPrefixList1"
                            }
                        }
                    }
                }
            ]
        },
        "testRoutingBGP": {
            "class": "RoutingBGP",
            "addressFamilies": [
                {
                    "internetProtocol": "ipv4",
                    "redistributionList": [
                        {
                            "routingProtocol": "kernel",
                            "routeMap": "testRouteMap"
                        }
                    ]
                }
            ],
            "gracefulRestart": {
                "gracefulResetEnabled": true,
                "restartTime": 120
            },
            "holdTime": 35,
            "keepAlive": 10,
            "localAS": 50208,
            "neighbors": [
                {
                    "address": "10.1.1.2",
                    "peerGroup": "Neighbor"
                }
            ],
            "peerGroups": [
                {
                    "name": "Neighbor",
                    "addressFamilies": [
                        {
                            "internetProtocol": "ipv4",
                            "routeMap": {
                                "out": "testRouteMap"
                            },
                            "softReconfigurationInboundEnabled": true
                        }
                    ],
                    "remoteAS": 65020
                }
            ],
            "routerId": "10.1.1.1"
        }
    }
}
