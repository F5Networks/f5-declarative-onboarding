{
    "schemaVersion": "1.0.0",
    "async": true,
    "class": "Device",
    "Common": {
        "class": "Tenant",
        "myVlan": {
            "class": "VLAN",
            "tag": 1234,
            "mtu": 1500,
            "interfaces": [
                {
                    "name": "1.1",
                    "tagged": true
                }
            ]
        },
        "internal": {
            "class": "VLAN",
            "tag": 4094,
            "interfaces": [
                {
                    "name": "1.1",
                    "tagged": false
                }
            ]
        },
        "mySelfIp": {
            "class": "SelfIp",
            "address": "10.148.75.46/24",
            "vlan": "myVlan",
            "allowService": [
                "tcp:80"
            ]
        },
        "myRoute": {
            "class": "Route",
            "gw": "10.148.75.1",
            "network": "10.20.30.0/24",
            "mtu": 1500
        },
        "myLocalOnlyRoute": {
            "class": "Route",
            "target": "myVlan",
            "network": "default",
            "localOnly": true
        },
        "internal-self": {
            "class": "SelfIp",
            "address": "10.1.20.31/32",
            "vlan": "internal",
            "allowService": "default",
            "trafficGroup": "traffic-group-local-only"
        },
        "int_rt": {
            "class": "Route",
            "gw": "10.1.20.1",
            "network": "10.1.20.0/24",
            "mtu": 1460
        },
        "int_gw_interface": {
            "class": "Route",
            "target": "internal",
            "network": "10.1.20.1/32",
            "mtu": 1460
        },
        "mySystem": {
            "class": "System",
            "autoPhonehome": false
        },
        "myResolver": {
            "class": "DNS_Resolver",
            "routeDomain": "0",
            "forwardZones": [
                {
                    "name": "forward.net",
                    "nameservers": [
                        "10.10.10.10:53"
                    ]
                }
            ]
        },
        "myMirror": {
            "class": "MirrorIp",
            "primaryIp": "10.148.75.46",
            "secondaryIp": "any6"
        },
        "testRoutingAsPath1": {
            "class": "RoutingAsPath",
            "entries": [
                {
                    "name": 10,
                    "regex": "^65001 *"
                }
            ]
        },
        "testRoutingAsPath2": {
            "class": "RoutingAsPath",
            "entries": [
                {
                    "name": 10,
                    "regex": "^$"
                },
                {
                    "name": 20,
                    "regex": "^65005$"
                }
            ]
        },
        "testRoutingPrefixList1": {
            "class": "RoutingPrefixList",
            "entries": [
                {
                    "name": 10,
                    "action": "permit",
                    "prefix": "1111:2222::/127",
                    "prefixLengthRange": 128
                }
            ]
        },
        "testRoutingPrefixList2": {
            "class": "RoutingPrefixList",
            "entries": [
                {
                    "name": 20,
                    "action": "permit",
                    "prefix": "10.3.3.0/24",
                    "prefixLengthRange": 32
                }
            ]
        }
    }
}
