        },
        "mySystem": {
            "class": "System",
            "autoPhonehome": false
        },
        "myResolver": {
            "class": "DNS_Resolver",
            "routeDomain": "0",
            "forwardZones": [
                {
                    "name": "forward.net",
                    "nameservers": [
                        "10.10.10.10:53"
                    ]
                }
            ]
        },
        "myMirror": {
            "class": "MirrorIp",
            "primaryIp": "10.148.75.46",
            "secondaryIp": "any6"
        }
    }
